{
  "name": "LBF_Earth_SourceCodeServer2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist & rimraf nccBuild & rimraf nexeBuild",
    "build": "nest build & npm run ncc:copy:views & npm run ncc:copy:public & npm run ncc:copy:env & npm run ncc:copy:dockerfile & npm run ncc:copy:package",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ncc:copy:env": "cpx .env ./dist",
    "ncc:copy:dockerfile": "cpx dockerfile ./dist",
    "ncc:copy:package": "cpx package.json ./dist",
    "ncc:copy:views": "cpx ./views/** ./dist/views",
    "ncc:copy:public": "cpx ./public/** ./dist/public",
    "ncc:copy:socket.io": "cpx ./node_modules/socket.io-client/dist/** ./nccBuild/node_modules/socket.io-client/dist & cpx .env ./nccBuild",
    "nexe:copy:socket.io": "cpx ./node_modules/socket.io-client/dist/** ./nexeBuild/node_modules/socket.io-client/dist & cpx .env ./nexeBuild",
    "nexe": "node nexeBuild.js & npm run nexe:copy:socket.io",
    "ncc": "ncc build ./dist/main.js -m -o ./nccBuild & npm run ncc:copy:socket.io",
    "nest:ncc": "npm run build & npm run ncc",
    "nest:ncc:nexe": "npm run build & npm run ncc & npm run nexe"
  },
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@nestjs/bull": "^0.2.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/microservices": "^7.4.4",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.4.4",
    "@nestjs/platform-ws": "^7.4.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "@nestjs/websockets": "^7.4.4",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@quickts/nestjs-log4js": "^1.0.5",
    "bull": "^3.18.0",
    "class-transformer": "0.3.1",
    "class-validator": "^0.12.2",
    "compressing": "^1.5.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "graphql-type-json": "^0.3.2",
    "http-proxy-middleware": "^2.0.0",
    "log4js": "^6.3.0",
    "moment": "^2.29.0",
    "mqtt": "^4.2.1",
    "nestjs-mailer": "^1.0.1",
    "nodemailer": "^6.4.13",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^6.5.4",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "@vercel/ncc": "~0.23.0",
    "apollo-server-express": "^2.18.1",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "nexe": "^3.3.7",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
